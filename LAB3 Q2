; Write a program which swaps the values of BL and CL registers without using XCHG instruction.
Values must be printed on separate lines [Note: create named constants to add new line in program]

.model small
.stack 100h
.data
.code

     main proc
         
        mov bl, 10
        
        mov cl, 25
        
        E Equ 0ah
        
        mov bh,bl
        mov bl,cl
        mov cl,bh
        
        mov ah,2
        mov dl,bl
        int 21h
        
        mov ah,2
        mov dl,E
        int 21h
        
        mov ah,2
        mov dl,cl
        int 21h
        
        
        
          
          mov ah,4ch
          int 21h
          
       main endp
       end main     
          
