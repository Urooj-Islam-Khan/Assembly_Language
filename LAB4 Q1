
.model small
.stack 100h
.data

    name db "Name: Urooj Islam $"
    fname dw "Father's Name: Islam Khan $"
    class db "Class: BS(SWE) Part-II $ "
    shift db "Shift: Morning $"
    
.code

     main proc
         
         mov ax,@data
         mov dx,ax
         
         mov ah,9
         LEA dx, name
         int 21h
         
         LEA dx, gap
         int 21h
         
         LEA dx,fname
         int 21h
         
         LEA dx, gap
         int 21h
          
         LEA dx, class
         int 21h
         
         LEA dx, gap
         int 21h
         
         LEA dx, shift
         int 21h
         
         
         
          
          mov ah,4ch
          int 21h
          
       main endp
       end main     
          
