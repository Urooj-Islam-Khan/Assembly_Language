; Write a program which takes a upper case as input and prints lowercase on new line with appropriate message.

.model small
.stack 100h
.data
.code

     main proc
         
          mov ah,1
          mov bl,al
          int 21h
          add bl,32
          
          mov ah,1
          mov bh,al
          int 21h
          add bh,32
          
          mov ah,1
          mov cl,al
          int 21h
          add cl,32
          
          mov ah,1
          mov ch,al
          int 21h
          add ch,32
          
          mov ah,1
          mov dh,al
          int 21h
          add dh,32
          
          mov ah,2
          mov dl,0ah
          int 21h
          
          mov ah,2
          mov dl,bl
          int 21h
          
          mov ah,2
          mov dl,bh
          int 21h
          
          mov ah,2
          mov dl,cl
          int 21h
          
          mov ah,2
          mov dl,ch
          int 21h
          
          mov ah,2
          mov dl,dh
          int 21h
          
          
          mov ah,4ch
          int 21h
          
       main endp
       end main     
