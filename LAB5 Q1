; Write a program which takes a lower case as input and prints upercase on new line with appropriate message.

.model small
.stack 100h
.data
.code

     main proc
         
          mov ah,1
          mov bl,al
          int 21h
          sub bl,32
          
          mov ah,1
          mov bh,al
          int 21h
          sub bh,32
          
          mov ah,1
          mov cl,al
          int 21h
          sub cl,32
          
          mov ah,1
          mov ch,al
          int 21h
          sub ch,32
          
          mov ah,1
          mov dh,al
          int 21h
          sub dh,32
          
          mov ah,2
          mov dl,0ah
          int 21h
          
          mov ah,2
          mov dl,bl
          int 21h
          
          mov ah,2
          mov dl,bh
          int 21h
          
          mov ah,2
          mov dl,cl
          int 21h
          
          mov ah,2
          mov dl,ch
          int 21h
          
          mov ah,2
          mov dl,dh
          int 21h
          
          
          mov ah,4ch
          int 21h
          
       main endp
       end main     
          
