; Write a Program which Swaps values of two variables.
.model small
.stack 100h
.data
val_1 '$'
val_2 '#'
temp ?
.code

     main proc
         
          mov dx,@data
          mov ds,dx
          
          mov bl,val_1
          mov cl,val_2
          mov bh,temp
          
          mov bh,bl
          mov bl,cl
          mov cl,bh
          
          mov ah,2
          mov dl,bl
          int 21h
          
          mov ah,2
          mov dl,0ah
          int 21h
          
          mov ah,2
          mov dl,cl
          int 21h
          
         
          mov ah,4ch
          int 21h
          
       main endp
       end main     
          
