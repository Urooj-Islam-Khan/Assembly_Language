;write a program that takes single character input, and prints next character on screen.

.model small
.stack 100h
.data
.code

     main proc
         
          mov ah,1
          mov bl,al
          int 21h
          
          mov cl,1
          int 21h
          
          ADD bl,cl
          
          mov ah,2
          mov dl,0ah
          int 21h
          
          mov ah,2
          mov dl,bl
          int 21h
          
          mov ah,4ch
          int 21h
          
       main endp
       end main     
          
