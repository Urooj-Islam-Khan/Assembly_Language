;Write a Program which takes single character as input and displays it on newline.

.model small
.stack 100h
.data
.code

     main proc
         
          mov ah,1
          mov al,bl
          int 21h
          
          mov dl,0ah
          int 21h
          
          mov ah,2
          mov dl,al
          int 21h 
          
          mov ah,4ch
          int 21h
          
       main endp
       end main  
